package com.writing.management.WPM.events;

import com.writing.documents.WritingPiece;

//TODO Sort out the structure and hierarchy of WPEVENT
public abstract class WPEvent {
	private WritingPiece writingPiece;
	
	public WPEvent(WritingPiece wp) {
		this.writingPiece = wp;
	}
	public WritingPiece getWritingPiece() {
		return writingPiece;
	}
}
